{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": true,
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "arrow-return-shorthand": true,
    "ordered-imports": false,
    "jsx-no-multiline-js": false
  },
  "rulesDirectory": []
}