{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": true,
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}